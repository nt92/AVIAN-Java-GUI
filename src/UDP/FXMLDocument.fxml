<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socketclientfx.FXMLDocumentController">
   <children>
      <HBox prefHeight="266.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox prefHeight="220.0" prefWidth="300.0" HBox.hgrow="ALWAYS">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label fx:id="testLabel" prefHeight="303.0" prefWidth="289.0" text="TEXTTTTTTTTTTTTTTTTTTT" />
                        <Button fx:id="done_traj_button" mnemonicParsing="false" onAction="#handleDoneTrajButton" text="Done" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
         <children>
            <Button fx:id="sendButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#handleSendMessageButton" text="Send" HBox.hgrow="NEVER" />
            <TextField fx:id="sendTextField" prefHeight="25.0" prefWidth="500.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="connectedLabel" text="Not connected">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
            <Button fx:id="connectButton" mnemonicParsing="false" onAction="#handleConnectButton" text="Connect">
               <font>
                  <Font size="10.0" />
               </font></Button>
            <Button fx:id="disconnectButton" mnemonicParsing="false" onAction="#handleDisconnectButton" text="Disconnect">
               <font>
                  <Font size="10.0" />
               </font></Button>
            <Label text="Host:">
               <font>
                  <Font size="10.0" />
               </font></Label>
            <TextField fx:id="hostTextField" prefHeight="25.0" prefWidth="200.0" text="localhost" />
            <Label text="Port:">
               <font>
                  <Font size="10.0" />
               </font></Label>
            <TextField fx:id="portTextField" minWidth="-Infinity" prefHeight="25.0" prefWidth="48.0" text="2015" />
         </children>
         <VBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
